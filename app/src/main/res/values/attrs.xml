<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="Theme">
        <attr name="title_text_color" format="reference|color"/>
        <attr name="sub_title_text_color" format="reference|color"/>
        <attr name="button_text_color" format="reference|color"/>
        <attr name="button_bg_selector" format="reference"/>
        <attr name="icon_launcher" format="reference"/>
        <attr name="icon_change_theme" format="reference"/>
        <attr name="img_cover_color" format="reference"/>
        <attr name="progress_indeterminate" format="reference"/>
        <attr name="progress_horizontal" format="reference"/>
        <attr name="title_text_style" format="reference"/>
    </declare-styleable>



    <declare-styleable name="RoundImageView">
        <!-- 描边宽度-->
        <attr name="border_width" format="dimension"/>
        <!-- 描边颜色-->
        <attr name="border_color" format="color"/>
        <!--圆角大小-->
        <attr name="corner_radius" format="dimension"/>
        <!--左上圆角大小-->
        <attr name="leftTop_corner_radius" format="dimension"/>
        <!--右上圆角大小-->
        <attr name="rightTop_corner_radius" format="dimension"/>
        <!--左下圆角大小-->
        <attr name="leftBottom_corner_radius" format="dimension"/>
        <!--右下圆角大小-->
        <attr name="rightBottom_corner_radius" format="dimension"/>
        <!--图片类型：圆角或者圆形-->
        <attr name="type" format="enum">
            <enum name="oval" value="2"/>
            <enum name="round" value="1"/>
            <enum name="circle" value="0"/>
        </attr>
    </declare-styleable>
</resources>